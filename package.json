{
  "name": "electron-actionhero",
  "productName": "ElectronActionhero",
  "version": "0.0.1",
  "description": "Bundling Actionhero with Electron",
  "license": "MIT",
  "repository": "semiosis/electron-actionhero",
  "author": {
    "name": "Louis Zuckerman",
    "email": "code@louiszuckerman.com",
    "url": "http://github.com/semiosis"
  },
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --app-version=$npm_package_version --platform=darwin --arch=x64 --prune --overwrite",
    "build-asar": "electron-packager . --out=dist --app-version=$npm_package_version --platform=darwin --arch=x64 --asar --prune --overwrite"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "actionhero": "semiosis/actionhero#electron-browserwindow",
    "electron-debug": "^1.0.0",
    "fs-extra": "^0.30.0",
    "node-resque": "semiosis/node-resque"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.0.1",
    "xo": "^0.15.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
